{
  "name": "accounting-backend",
  "description": "simple accounting backend",
  "version": "0.0.7",
  "author": "Emilio Platzer <emilioplatzer@gmail.com>",
  "license": "MIT",
  "repository": "emilioplatzer/accounting-backend",
  "main": "server/accounting-server.js",
  "entrypoint": "server/accounting-server.js",
  "dependencies": {
    "es6-shim": "~0.35.0",
    "es6-transpiler": "~0.7.18",
    "fs-extra": "~0.26.7",
    "fs-promise": "~0.5.0",
    "jade": "~1.11.0",
    "lodash": "~4.6.1",
    "read-yaml-promise": "~1.0.2",
    "stylus": "~0.54.2",

    "accounting-machine": "~0.0.8",
    "ajax-best-promise": "~0.0.7",
    "backend-plus": "~0.0.9",
    "best-globals": "~0.2.4",
    "best-promise": "~0.2.4",
    "extension-serve-static": "~0.1.6",
    "js-to-html": "~0.9.0",
    "mini-tools": "~0.2.5",
    "promise-plus": "~0.0.6",
    "tedede": "~0.2.0"
  },
  "devDependencies": {
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5"
  },
  "optionalDependencies": {
    "kill-9": "0.3.0"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "postinstall": "node node_modules/es6-transpiler/build/es5/es6toes5.js server/accounting-server.js > server-trans/accounting-server.js",
    "start": "node triage-esV.js"
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true,
    "esversion": 6
  },
  "eslintConfig": {
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "qa-control": {
    "package-version": "0.1.4",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "app"
  }
}
