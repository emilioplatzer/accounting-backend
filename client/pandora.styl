body
  margin 0px
  padding 0px
  background #EEE
  font-family Arial

#superior-y-lateral
  height 24px
  border-bottom 1px solid #999
  background-color #DDD

#superior
  display inline-block
  padding 3px
  vertical-align top
  
#lateral
  width 24px
  height 100%
  border-right 1px solid #999
  background-color #DDD
  float left
  
#central
  height 100%
  background #EF8

.linea-menu
  margin 16px
  margin-left 40px
  vertical-align middle

.linea-menu a
  text-decoration none

.linea-menu img
  vertical-align -30%
  
.linea-menu span
  margin 6px
  font-size 110%
  color black
  vertical-align middle
  